<?xml version='1.0' encoding='utf-8'?>
<widget id="com.esferasoluciones.mymat_light" version="8.2.2" versionCode="56" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>MyMat - Light</name>
    <description>MyMat 8</description>
    <author email="lsanchez@esferasoluciones.com" href="https://www.esferasoluciones.com">
        Luis Ricardo SÃ¡nchez.
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="ScrollEnabled" value="false" />
    <preference name="orientation" value="portrait" />
    <preference name="android-minSdkVersion" value="23" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="FadeSplashScreenDuration" value="300" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="3000" />
    <preference name="phonegap-version" value="cli-8.0.0" />
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
    <preference name="CordovaWebViewEngine" value="CDVUIWebViewEngine" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="resources/android/icon/ldpi.png" />
        <icon density="mdpi" src="resources/android/icon/mdpi.png" />
        <icon density="hdpi" src="resources/android/icon/hdpi.png" />
        <icon density="xhdpi" src="resources/android/icon/xhdpi.png" />
        <icon density="xxhdpi" src="resources/android/icon/xxhdpi.png" />
        <icon density="xxxhdpi" src="resources/android/icon/xxxhdpi.png" />
        <splash density="port-ldpi" src="resources/android/splash/splash-port-ldpi.png" />
        <splash density="port-mdpi" src="resources/android/splash/splash-port-mdpi.png" />
        <splash density="port-hdpi" src="resources/android/splash/splash-port-hdpi.png" />
        <splash density="port-xhdpi" src="resources/android/splash/splash-port-xhdpi.png" />
        <splash density="port-xxhdpi" src="resources/android/splash/splash-port-xxhdpi.png" />
        <splash density="port-xxxhdpi" src="resources/android/splash/splash-port-xxxhdpi.png" />
        <preference name="loadUrlTimeoutValue" value="700000" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon height="16" src="resources/ios/icon/icon-16@1x.png" width="16" />
        <icon height="32" src="resources/ios/icon/icon-16@2x.png" width="32" />
        <icon height="48" src="resources/ios/icon/icon-16@3x.png" width="48" />
