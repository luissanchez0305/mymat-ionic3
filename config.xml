        <icon height="50" src="resources/ios/icon/icon-50@1x.png" width="50" />
        <icon height="100" src="resources/ios/icon/icon-50@2x.png" width="100" />
        <icon height="55" src="resources/ios/icon/icon-55@1x.png" width="55" />
